{% import 'types.tpl' as types with context %}


/**
 * @constructor
 * @implements {{'{' + data.namespace }}.model.I{{ structure.name }}}
 *
 {% for field in structure.fields -%}
   * @param {{ '{' + types.printJs(field) + '}' }} {{ field.name  }}
 {% endfor -%}
 */
{{ data.namespace }}.model.{{ structure.name }} = 
    function(
    {%- for field in structure.fields -%}
      {% if not loop.first %}, {% endif %}
      {{ field.name }}
    {%- endfor %}) {

  {%- for field in structure.fields %}

  /**
   * @type {{ '{' + types.printJs(field) + '}' }}
   */
  this.{{ field.name }} = {{ field.name  }};
  {%- endfor %}
};


/**
 * @inheritDoc
 */
{{ data.namespace }}.model.{{ structure.name }}.prototype.toThrift =
  function() {
    return this;
};
