{% import 'types.tpl' as types with context %}


/**
 * @constructor
 * @implements {{'{' + data.namespace }}.model.IBuilder}
 */
{{ data.namespace }}.model.Builder = function() {
};

{% for structure in data.structures %}


/**
 * @inheritDoc
 */
{{ data.namespace }}.model.Builder.prototype.create{{ structure.name }} =
function(
 {%- for field in structure.fields -%}
      {% if not loop.first %}, {% endif %}
      {{ field.name }}
    {%- endfor %}){

  return new {{ data.namespace }}.model.{{ structure.name }}(
  {%- for field in structure.fields -%}
        {% if not loop.first %}, {% endif %}
        {{ field.name }}
      {%- endfor %});
};
{% endfor %}
